{
  "name": "aura-expression",
  "displayName": "aura-expression",
  "description": "aura expression syntax",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "configuration":{
      "enable": true
    },
    "languages": [
			{
				"id": "aura",
				"extensions": [
					".cmp"
				],
				"configuration": "./language-configuration.json"
			}
		],
    "grammars": [
      {
        "language": "aura",
        "scopeName": "source.aura",
        "path": "./syntaxes/html.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
